<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.github.fragoi.autobright.Debug">
    <method name="Enable">
      <arg name="value1" type="b" direction="in" />
      <arg name="value2" type="b" direction="in" />
      <arg name="res" type="s" direction="out" />
    </method>
    <method name="Disable" />
    <signal name="LightLevelChanged">
      <arg name="value" type="d" />
    </signal>
    <property name="Bar" type="s" access="readwrite" />
  </interface>
</node>